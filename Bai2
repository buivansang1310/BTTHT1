import java.util.Random;

public class Main {
    public static void main(String[] args) {
        int n = 100000; 
        int c = 0;
        Random r = new Random();

        for (int i = 0; i < n; i++) {
            double x = r.nextDouble();
            double y = r.nextDouble();
            if (x * x + y * y <= 1) c++;
        }

        System.out.println(4.0 * c / n);
    }
}
